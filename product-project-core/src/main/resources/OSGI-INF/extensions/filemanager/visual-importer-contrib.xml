<?xml version="1.0"?>
<component name="org.nuxeo.onboarding.exercise.extensions.filemanager.visualimporter">

    <extension point="plugins" target="org.nuxeo.ecm.platform.filemanager.service.FileManagerService">
        <documentation>
            Simple plugin for the file manager. Creates a Visual Document type from any of the matching mime types.
        </documentation>
        <plugin class="org.nuxeo.onboarding.exercise.extensions.filemanager.NxVisualImporter" name="NxVisualImporter" order="1">
            <filter>image/png</filter>
        </plugin>
    </extension>

</component>
